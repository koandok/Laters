<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Laters</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
    <filter>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>com.gxuwz.laters.web.filter.CharacterEncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>BuildServlet</display-name>
    <servlet-name>BuildServlet</servlet-name>
    <servlet-class>com.gxuwz.laters.web.servlet.BuildServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DepServlet</servlet-name>
    <servlet-class>com.gxuwz.laters.web.servlet.DepServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DormServlet</servlet-name>
    <servlet-class>com.gxuwz.laters.web.servlet.DormServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.gxuwz.laters.web.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LaterServlet</servlet-name>
    <servlet-class>com.gxuwz.laters.web.servlet.LaterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ExcServlet</servlet-name>
    <servlet-class>com.gxuwz.laters.bean.manager.ExcServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>BuildServlet</servlet-name>
    <url-pattern>/BuildServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DepServlet</servlet-name>
    <url-pattern>/DepServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DormServlet</servlet-name>
    <url-pattern>/DormServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LaterServlet</servlet-name>
    <url-pattern>/LaterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ExcServlet</servlet-name>
    <url-pattern>/ExcServlet</url-pattern>
  </servlet-mapping>
</web-app>